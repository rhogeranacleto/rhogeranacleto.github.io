{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"isolatedModules": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"declaration": false,
		"noImplicitAny": true,
		"removeComments": true,
		"noLib": false,
		"preserveConstEnums": true,
		"suppressImplicitAnyIndexErrors": true
	},
	"compileOnSave": false,
	"files": [
		"scripts/*.ts"
	]
}